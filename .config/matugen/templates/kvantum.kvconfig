[GeneralColors]
window.color={{colors.surface.default.hex}}
base.color={{colors.secondary.default.hex}}
alt.base.color={{colors.surface_bright.default.hex}}
button.color={{colors.surface_container_high.default.hex}}
light.color={{colors.surface_bright.default.hex}}
mid.light.color={{colors.surface_container_high.default.hex}}
dark.color={{colors.surface_dim.default.hex}}
mid.color={{colors.surface_container.default.hex}}
highlight.color={{colors.primary.default.hex}}
inactive.highlight.color={{colors.outline_variant.default.hex}}
text.color={{colors.on_secondary.default.hex}}
window.text.color={{colors.on_surface.default.hex}}
button.text.color={{colors.on_surface.default.hex}}
disabled.text.color={{colors.outline.default.hex}}
tooltip.text.color={{colors.on_surface.default.hex}}
highlight.text.color={{colors.on_primary.default.hex}}
link.color={{colors.primary.default.hex}}
link.visited.color={{colors.tertiary.default.hex}}
progress.indicator.text.color={{colors.on_primary.default.hex}}

[Hacks]
transparent_ktitle_label=true
transparent_dolphin_view=false
transparent_pcmanfm_sidepane=true
blur_translucent=true
transparent_menutitle=true
respect_darkness=false
kcapacitybar_as_progressbar=true
force_size_grip=true
iconless_pushbutton=false
iconless_menu=false
disabled_icon_opacity=70
lxqtmainmenu_iconsize=22
normal_default_pushbutton=true
single_top_toolbar=true
tint_on_mouseover=0
transparent_pcmanfm_view=false
no_selection_tint=false

[PanelButtonCommand]
frame=true
frame.element=button
frame.top=3
frame.bottom=3
frame.left=3
frame.right=3
interior=true
interior.element=button
indicator.size=8
text.normal.color={{colors.on_surface.default.hex}}
text.focus.color={{colors.on_primary.default.hex}}
text.press.color={{colors.on_primary.default.hex}}
text.toggle.color={{colors.on_primary.default.hex}}
text.shadow=0
text.margin=1
text.iconspacing=4
indicator.element=arrow
text.margin.top=2
text.margin.bottom=2
text.margin.left=4
text.margin.right=4
frame.expansion=0

[PanelButtonTool]
inherits=PanelButtonCommand
text.normal.color={{colors.on_surface.default.hex}}
text.focus.color={{colors.on_primary.default.hex}}
text.press.color={{colors.on_primary.default.hex}}
text.toggle.color={{colors.on_primary.default.hex}}
indicator.element=arrow

[ToolbarButton]
inherits=PanelButtonCommand
frame.element=tbutton
interior.element=tbutton
indicator.element=arrow

[Toolbar]
inherits=PanelButtonCommand
frame.element=none
interior.element=none

[Menu]
frame.top=3
frame.bottom=3
frame.left=3
frame.right=3
frame.element=menu
interior.element=menu
text.normal.color={{colors.on_surface.default.hex}}
text.focus.color={{colors.on_primary.default.hex}}
text.press.color={{colors.on_primary.default.hex}}
text.toggle.color={{colors.on_primary.default.hex}}

[MenuItem]
frame=true
frame.element=menuitem
interior=true
interior.element=menuitem
text.normal.color={{colors.on_surface.default.hex}}
text.focus.color={{colors.on_primary.default.hex}}
text.press.color={{colors.on_primary.default.hex}}
text.toggle.color={{colors.on_primary.default.hex}}
indicator.element=menuitem

[ItemView]
inherits=PanelButtonCommand
text.margin=0
frame.element=itemview
interior.element=itemview
frame.top=2
frame.bottom=2
frame.left=2
frame.right=2
text.margin.top=2
text.margin.bottom=2
text.margin.left=4
text.margin.right=4
text.normal.color={{colors.on_surface.default.hex}}
text.focus.color={{colors.on_surface.default.hex}}
text.press.color={{colors.on_primary.default.hex}}
text.toggle.color={{colors.on_primary.default.hex}}
min_width=+0.3font
min_height=+0.3font
frame.expansion=0

[HeaderSection]
inherits=PanelButtonCommand
interior.element=header
frame.element=header
frame.top=1
frame.bottom=1
text.normal.color={{colors.on_surface.default.hex}}
text.focus.color={{colors.on_primary.default.hex}}
text.press.color={{colors.on_primary.default.hex}}
text.toggle.color={{colors.on_primary.default.hex}}

[TreeExpander]
inherits=PanelButtonCommand
indicator.size=12
indicator.element=tree
text.normal.color={{colors.on_surface.default.hex}}
