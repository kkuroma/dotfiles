{
    "editor.fontFamily": "'Maple Mono NF CN', 'Noto Sans Mono', monospace",
    "window.zoomLevel": 0,
    "editor.fontSize": 14,
    "terminal.integrated.fontFamily": "'Maple Mono NF CN', 'Noto Sans Mono', monospace",
    "workbench.colorCustomizations": {
        "editor.background": "#14140c",
        "editor.foreground": "#e6e2d5",
        "editorBracketMatch.background": "#4a490040",
        "editorBracketMatch.border": "#cdca75",
        "activityBar.background": "#212018",
        "activityBar.foreground": "#e6e2d5",
        "activityBar.inactiveForeground": "#cac7b6",
        "activityBarBadge.background": "#cdca75",
        "activityBarBadge.foreground": "#333200",
        "sideBar.background": "#1c1c14",
        "sideBar.foreground": "#e6e2d5",
        "sideBarTitle.foreground": "#e6e2d5",
        "sideBarSectionHeader.background": "#212018",
        "sideBarSectionHeader.foreground": "#e6e2d5",
        "list.activeSelectionBackground": "#cdca75",
        "list.activeSelectionForeground": "#333200",
        "list.inactiveSelectionBackground": "#2b2a22",
        "list.hoverBackground": "#36352c",
        "statusBar.background": "#212018",
        "statusBar.foreground": "#e6e2d5",
        "statusBar.noFolderBackground": "#212018",
        "titleBar.activeBackground": "#212018",
        "titleBar.activeForeground": "#e6e2d5",
        "titleBar.inactiveBackground": "#14140c",
        "titleBar.inactiveForeground": "#cac7b6",
        "tab.activeBackground": "#2b2a22",
        "tab.activeForeground": "#e6e2d5",
        "tab.inactiveBackground": "#14140c",
        "tab.inactiveForeground": "#cac7b6",
        "tab.border": "#48473a",
        "editorGroupHeader.tabsBackground": "#14140c",
        "editorLineNumber.foreground": "#939181",
        "editorLineNumber.activeForeground": "#cdca75",
        "editorCursor.foreground": "#cdca75",
        "editor.selectionBackground": "#4a490040",
        "editor.lineHighlightBackground": "#2b2a22",
        "editorWidget.background": "#2b2a22",
        "editorWidget.border": "#939181",
        "editorSuggestWidget.background": "#2b2a22",
        "editorSuggestWidget.selectedBackground": "#cdca75",
        "input.background": "#36352c",
        "input.foreground": "#e6e2d5",
        "input.border": "#939181",
        "inputOption.activeBorder": "#cdca75",
        "focusBorder": "#cdca75",
        "button.background": "#cdca75",
        "button.foreground": "#333200",
        "button.hoverBackground": "#4a4900",
        "dropdown.background": "#36352c",
        "dropdown.border": "#939181",
        "panel.background": "#14140c",
        "panel.border": "#48473a",
        "panelTitle.activeForeground": "#cdca75",
        "terminal.foreground": "#e6e2d5",
        "terminal.background": "#14140c",
        "terminal.ansiBlack": "#262626",
        "terminal.ansiBrightBlack": "#4c4c4c",
        "terminal.ansiRed": "#ac8a8c",
        "terminal.ansiBrightRed": "#c49ea0",
        "terminal.ansiGreen": "#8aac8b",
        "terminal.ansiBrightGreen": "#9ec49f",
        "terminal.ansiYellow": "#aca98a",
        "terminal.ansiBrightYellow": "#c4c19e",
        "terminal.ansiBlue": "#cdca75",
        "terminal.ansiBrightBlue": "#4a4900",
        "terminal.ansiMagenta": "#ac8aac",
        "terminal.ansiBrightMagenta": "#c49ec4",
        "terminal.ansiCyan": "#8aacab",
        "terminal.ansiBrightCyan": "#9ec3c4",
        "terminal.ansiWhite": "#e7e7e7",
        "terminal.ansiBrightWhite": "#f0f0f0",
        "terminal.selectionBackground": "#cbc8a4",
        "terminalCursor.foreground": "#e6e2d5",
        "terminalCursor.background": "#cac7b6",
        "scrollbarSlider.background": "#36352c80",
        "scrollbarSlider.hoverBackground": "#36352c",
        "scrollbarSlider.activeBackground": "#cdca7580"
    },
    "editor.tokenColorCustomizations": {
        "comments": "#8a8a8a",
        "keywords": "#cdca75",
        "strings": "#a8c49e",
        "numbers": "#c4a89e",
        "functions": "#cbc8a4",
        "types": "#9ec4c1",
        "variables": "#e6e2d5",
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#8a8a8a",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "keyword",
                    "storage.type",
                    "storage.modifier",
                    "keyword.control",
                    "keyword.operator.new"
                ],
                "settings": {
                    "foreground": "#cdca75"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "support.function",
                    "meta.function-call"
                ],
                "settings": {
                    "foreground": "#cbc8a4"
                }
            },
            {
                "scope": [
                    "entity.name.type",
                    "entity.name.class",
                    "support.class",
                    "support.type"
                ],
                "settings": {
                    "foreground": "#9ec4c1"
                }
            },
            {
                "scope": [
                    "string",
                    "string.quoted",
                    "string.template"
                ],
                "settings": {
                    "foreground": "#a8c49e"
                }
            },
            {
                "scope": [
                    "constant.numeric",
                    "constant.language.numeric"
                ],
                "settings": {
                    "foreground": "#c4a89e"
                }
            },
            {
                "scope": [
                    "constant.language",
                    "constant.language.boolean",
                    "constant.language.null"
                ],
                "settings": {
                    "foreground": "#c49ea0"
                }
            },
            {
                "scope": [
                    "variable",
                    "variable.other.readwrite",
                    "variable.other.object"
                ],
                "settings": {
                    "foreground": "#e6e2d5"
                }
            },
            {
                "scope": [
                    "variable.parameter",
                    "variable.other.constant"
                ],
                "settings": {
                    "foreground": "#b8acb0"
                }
            },
            {
                "scope": [
                    "entity.name.tag",
                    "meta.tag",
                    "punctuation.definition.tag"
                ],
                "settings": {
                    "foreground": "#cdca75"
                }
            },
            {
                "scope": [
                    "entity.other.attribute-name"
                ],
                "settings": {
                    "foreground": "#cbc8a4",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "punctuation.definition.string",
                    "punctuation.definition.template-expression"
                ],
                "settings": {
                    "foreground": "#a8c49e"
                }
            },
            {
                "scope": [
                    "support.constant",
                    "constant.other"
                ],
                "settings": {
                    "foreground": "#aca8c4"
                }
            },
            {
                "scope": [
                    "markup.heading"
                ],
                "settings": {
                    "foreground": "#cdca75",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "markup.bold"
                ],
                "settings": {
                    "foreground": "#cbc8a4",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "markup.italic"
                ],
                "settings": {
                    "foreground": "#a8c49e",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "meta.import",
                    "meta.require"
                ],
                "settings": {
                    "foreground": "#b8acb0"
                }
            },
            {
                "scope": [
                    "variable.language.this",
                    "variable.language.super"
                ],
                "settings": {
                    "foreground": "#c49ea0",
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "window.dialogStyle": "native",
    "window.titleBarStyle": "custom",
    "files.autoSave": "afterDelay"
}