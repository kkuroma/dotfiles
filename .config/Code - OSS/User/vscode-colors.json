    "editor.tokenColorCustomizations": {
        "comments": "#8a8a8a",
        "keywords": "#ffb2bb",
        "strings": "#a8c49e",
        "numbers": "#c4a89e",
        "functions": "#e5bdc0",
        "types": "#9ec4c1",
        "variables": "#f0dedf",
        "textMateRules": [
            {
                "scope": ["comment", "punctuation.definition.comment"],
                "settings": {
                    "foreground": "#8a8a8a",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control", "keyword.operator.new"],
                "settings": {
                    "foreground": "#ffb2bb"
                }
            },
            {
                "scope": ["entity.name.function", "support.function", "meta.function-call"],
                "settings": {
                    "foreground": "#e5bdc0"
                }
            },
            {
                "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
                "settings": {
                    "foreground": "#9ec4c1"
                }
            },
            {
                "scope": ["string", "string.quoted", "string.template"],
                "settings": {
                    "foreground": "#a8c49e"
                }
            },
            {
                "scope": ["constant.numeric", "constant.language.numeric"],
                "settings": {
                    "foreground": "#c4a89e"
                }
            },
            {
                "scope": ["constant.language", "constant.language.boolean", "constant.language.null"],
                "settings": {
                    "foreground": "#c49ea0"
                }
            },
            {
                "scope": ["variable", "variable.other.readwrite", "variable.other.object"],
                "settings": {
                    "foreground": "#f0dedf"
                }
            },
            {
                "scope": ["variable.parameter", "variable.other.constant"],
                "settings": {
                    "foreground": "#b8acb0"
                }
            },
            {
                "scope": ["entity.name.tag", "meta.tag", "punctuation.definition.tag"],
                "settings": {
                    "foreground": "#ffb2bb"
                }
            },
            {
                "scope": ["entity.other.attribute-name"],
                "settings": {
                    "foreground": "#e5bdc0",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["punctuation.definition.string", "punctuation.definition.template-expression"],
                "settings": {
                    "foreground": "#a8c49e"
                }
            },
            {
                "scope": ["support.constant", "constant.other"],
                "settings": {
                    "foreground": "#aca8c4"
                }
            },
            {
                "scope": ["markup.heading"],
                "settings": {
                    "foreground": "#ffb2bb",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": ["markup.bold"],
                "settings": {
                    "foreground": "#e5bdc0",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": ["markup.italic"],
                "settings": {
                    "foreground": "#a8c49e",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["meta.import", "meta.require"],
                "settings": {
                    "foreground": "#b8acb0"
                }
            },
            {
                "scope": ["variable.language.this", "variable.language.super"],
                "settings": {
                    "foreground": "#c49ea0",
                    "fontStyle": "italic"
                }
            }
        ]
    }
}
